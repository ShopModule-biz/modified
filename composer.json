{
  "name": "heidelpay/modified",
  "description": "This extension for modified eCommerce provides a direct integration of the heidelpay payment methods to your modified shop.",
  "require": {
    "php": ">=5.6.0"
  },
  "require-dev": {
    "heidelpay/phpdocumentor": "2.9.1",
    "friendsofphp/php-cs-fixer": "2.1.0",
    "slowprog/composer-copy-file": "~0.2"
  },
  "type": "modified-extension",
  "license": "Use of this software requires acceptance of the Evaluation License Agreement. See LICENSE file.",
  "autoload": {
    "psr-4": {
      "Heidelpay\\Modified\\": ""
    }
  },
  "support": {
    "email": "support@heidelpay.de"
  },
  "homepage": "https://dev.heidelpay.de",
  "authors": [
    {
      "name": "Jens Richter",
      "email": "development@heidelpay.de",
      "homepage": "https://dev.heidelpay.de/magento2"
    }
  ],
  "scripts": {
    "post-install-cmd": [
      "SlowProg\\CopyFile\\ScriptHandler::copy"
    ],
    "post-update-cmd": [
      "SlowProg\\CopyFile\\ScriptHandler::copy"
    ]
  },
  "extra": {
    "copy-file": {
      "admin/includes/modules/export/": "admin/include/modules/export/",
      "images/ladebalken.gif": "images/ladebalken.gif",
      "includes/classes/class.heidelpay.php": "includes/classes/class.heidelpay.php",
      "includes/modules/payment/": "includes/modules/payment/",
      "lang/english/modules/payment/": "lang/english/modules/payment/",
      "lang/german/modules/payment/": "lang/german/modules/payment/",
      "heidelpay_3dsecure.php": "heidelpay_3dsecure.php",
      "heidelpay_3dsecure_return.php": "heidelpay_3dsecure_return.php",
      "heidelpay_after_register.php": "heidelpay_after_register.php",
      "heidelpay_checkout_iframe.php": "heidelpay_checkout_iframe.php",
      "heidelpay_gm_checkout_iframe.php": "heidelpay_gm_checkout_iframe.php",
      "heidelpay_iframe.php": "heidelpay_iframe.php",
      "heidelpay_redirect.php": "heidelpay_redirect.php",
      "heidelpay_reg_style.css": "heidelpay_reg_style.css",
      "heidelpay_style.css": "heidelpay_style.css",
      "heidelpay_success.inc.php": "heidelpay_success.inc.php",
      "heidelpay_success.php": "heidelpay_success.php",
      "LICENSE.txt": "includes/external/heidelpay/",
      "README.md": "includes/external/heidelpay/"
    }
  },
  "keywords": [
    "modified",
    "modified eCommerce",
    "xtc-fork",
    "php",
    "payment-gateway"
  ]
}